{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**/AboutUs",
        "destination": "/AboutUs.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
